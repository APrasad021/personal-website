{
  "name": "personal-website-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@vercel/analytics": "^0.1.6",
    "firebase": "^9.10.0",
    "graphql": "^16.6.0",
    "moment": "^2.29.4",
    "next": "12.1.6",
    "react": "18.2.0",
    "react-device-detect": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-rating": "^2.0.5",
    "ua-parser-js": "^1.0.33"
  },
  "devDependencies": {
    "@types/node": "17.0.40",
    "@types/react": "18.0.12",
    "@types/react-dom": "18.0.5",
    "eslint": "8.17.0",
    "eslint-config-next": "12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "pre-commit": "^1.2.2",
    "prettier": "2.7.1",
    "typescript": "4.7.3"
  },
  "pre-commit": [
    "lint",
    "check-format"
  ]
}
